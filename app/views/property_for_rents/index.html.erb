<div class="container">
  <h4 class="text-center">Listing property_for_rents</h4>
  <div align="right"><a href="<%= new_property_for_rent_path%>" class="btn btn-primary"><span class="fui-plus"></span> New a Property_For_Rent</a></div>

  <table id="tablaDatos" class="table table-striped">
    <thead>
    <tr>
      <th>Pno</th>
      <th>Street</th>
      <th>Area</th>
      <th>City</th>
      <th>Pcode</th>
      <th>Type</th>
      <th>Rooms</th>
      <th>Rent</th>
      <th>Owner</th>
      <th>Staff</th>
      <th>Branch</th>
      <th></th>
    </tr>
    </thead>

    <tbody>
    <% @property_for_rents.each do |property_for_rent| %>
        <tr>
          <td><%= property_for_rent.Pno %></td>
          <td><%= property_for_rent.Street %></td>
          <td><%= property_for_rent.Area %></td>
          <td><%= property_for_rent.City %></td>
          <td><%= property_for_rent.Pcode %></td>
          <td><%= property_for_rent.Type %></td>
          <td><%= property_for_rent.Rooms %></td>
          <td><%= property_for_rent.Rent %></td>
          <td><%= property_for_rent.Ono %></td>
          <td><%= property_for_rent.Sno %></td>
          <td><%= property_for_rent.Bno %></td>
          <td>
            <ul class="col-md-1"><%= link_to '<span class="fui-new" aria-hidden="true"></span>'.html_safe, edit_property_for_rent_path(property_for_rent) %></ul>
            <ul class="col-md-1"><%= link_to '<span class="fui-trash" aria-hidden="true"></span>'.html_safe, property_for_rent, method: :delete, data: {confirm: 'Are you sure?'} %></ul>
          </td>
        </tr>
    <% end %>
    </tbody>
  </table>


</div>